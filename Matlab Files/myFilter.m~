function clean_audio = myFilter(window_array, audio)
   
    clean_audio_FT = fft(audio,length(audio));

    [r,c] = size(window_array);
    for i =1:c
        i
        clean_audio_FT
        clean_audio_FT = clean_audio_FT.*fft(window_array(:,i),length(audio));
    end
    


    % Allocate window array for speed.
    window_FT = zeros(length(window_array),length(audio));
    
    zeros(length(window_array));
    for i =length(window_array)

        audio_FT = fft(audio,length(audio));
        window_FT(i) = fft(window_array(:,1),length(audio)) ;
        clean_audio_FT = audio_FT.*window_FT(i);
        
    end
    
    clean_audio = ifft(clean_audio_FT);
end
